Simulate System Stack

// System Stack
int F(int n) {
	if(n <= 1) {
		return 1;
	}
	return n * F(n - 1);
}

// Stack by myself
boolean back = false;
int ret = 0;
do {
	if(!back) {
		if(n <= 1) {
			back = true;
			ret = 1;
			continue;
		}
		n enter Stack;
		--n;
	} else {
		ret *=  out Stack;
	}
} while(Stack not empty);